{
    "name": "copyaslist",
    "displayName": "CopyAsList",
    "description": "Copy multi-selection as list",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.copyAsList",
        "onCommand:extension.copyAsListOfStrings"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyAsList",
                "title": "Copy As List"
            },
            {
                "command": "extension.copyAsListOfStrings",
                "title": "Copy As List Of Strings"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/copy-paste": "^1.1.30",
        "copy-paste": "^1.3.0"
    }
}
